# TCELL USB3.2 4K FIRE thumb drive
# https://www.tcelltc.com.tw/ec99/rwd1085/product.asp?prodid=TC-048-003&category_id=63&cty_id=63
ACTION=="add|change", ATTRS{idVendor}=="090c", ATTRS{idProduct}=="3282", SUBSYSTEM=="scsi_disk", ATTR{provisioning_mode}="unmap"
